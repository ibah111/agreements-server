{
  "name": "agreements-server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "migrate:excel": "ts-node scripts/ImportFromExcel.ts",
    "migrate:excel:debug": "nodemon --inspect scripts/ImportFromExcel.ts",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "calcs": "nodemon --inspect scripts/test.ts",
    "commitlint": "commitlint --edit",
    "postinstall": "husky install",
    "prepare": "husky install"
  },
  "dependencies": {
    "@casl/ability": "^6.5.0",
    "@contact/models": "^0.3.74",
    "@fastify/static": "^6.11.2",
    "@mui/x-data-grid-premium": "^6.15.0",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/common": "^10.2.6",
    "@nestjs/core": "^10.2.6",
    "@nestjs/platform-fastify": "^10.2.6",
    "@nestjs/platform-socket.io": "^10.2.6",
    "@nestjs/schedule": "^3.0.4",
    "@nestjs/swagger": "^7.1.12",
    "@nestjs/terminus": "^10.1.1",
    "@nestjs/websockets": "^10.2.6",
    "@sql-tools/nestjs-sequelize": "^10.0.0",
    "@sql-tools/sequelize": "^6.32.0",
    "@sql-tools/sequelize-typescript": "^2.1.5",
    "@tools/terminus-indicators": "^1.0.3",
    "axios": "^1.5.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "colors": "^1.4.0",
    "exceljs": "^4.3.0",
    "lint-staged": "^14.0.1",
    "lodash": "^4.17.21",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "sqlite3": "^5.1.6",
    "tedious": "^16.4.1",
    "umzug": "^3.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@nestjs/cli": "^10.1.18",
    "@nestjs/schematics": "^10.0.2",
    "@sql-tools/association-literal": "^0.0.3",
    "@types/cron": "^2.4.0",
    "@types/express": "^4.17.18",
    "@types/lodash": "^4.14.199",
    "@types/node": "20.6.5",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "commander": "^11.0.0",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "source-map-support": "^0.5.21",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.2.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "packageManager": "yarn@3.6.3"
}
